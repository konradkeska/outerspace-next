  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#7F7F7F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Flow Typedef",
      "scope": "meta.type.flowtype variable.other.flowtype",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, storage.type, storage.type.function, variable.other.flowtype, storage.type.extends, storage.type.function, storage.type.class",
      "settings": {
        "foreground": "#7F7F7F"
      }
    },
    {
      "name": "Function Call",
      "scope": "variable.function, meta.class meta.group.braces.curly meta.function-call variable.function, entity.name.tag, variable.function.constructor",
      "settings": {
        "foreground": "#FFDE9E"
      }
    },
    {
      "name": "Function/Class Name",
      "scope": "entity.name.function, meta.class entity.name.class, meta.class meta.function-call variable.function, source keyword.control.loop, storage.type.function.arrow",
      "settings": {
        "foreground": "#F2969D"
      }
    },
    {
      "name": "Control Flow",
      "scope": "keyword.control.flow, keyword.control.trycatch, source meta.group.braces.curly keyword.control.loop, keyword.control.switch",
      "settings": {
        "foreground": "#F2969D"
      }
    },
    {
      "name": "Assignment",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#FFDE9E"
      }
    },
    {
      "name": "Logic",
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#FFDE9E"
      }
    },
    {
      "name": "Comparison",
      "scope": "keyword.operator.comparison, keyword.operator.relational",
      "settings": {
        "foreground": "#F2969D"
      }
    },
    {
      "name": "String",
      "scope": "string.quoted, string.interpolated",
      "settings": {
        "foreground": "#A6CEA1"
      }
    },
    {
      "name": "Literal",
      "scope": "constant.language, constant.numeric, support.constant, constant.character, constant.other.color, constant.other.symbol, constant.other.key, keyword.other.unit",
      "settings": {
        "foreground": "#83C2D3"
      }
    },

    {
      "name": "Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A6CEA1"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#E15A60"
      }
    },
    {
      "name": "Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#BB80B3"
      }
    },
    {
      "name": "URL",
      "scope": "*url*, *link*, *uri*",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Search Results Nums",
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#AB7967"
      }
    },
    {
      "name": "Search Results Lines",
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#A6CEA1"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "scope": "punctuation.definition.interpolation.begin.bracket.curly.scss,punctuation.definition.interpolation.end.bracket.curly.scss",
      "settings": {
        "foreground": "#7F7F7F"
      }
    },
    // C#
    {
      "scope": "keyword.other.this.cs",
      "settings": {
        "foreground": "#d4d4d4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.type.cs, entity.name.type.class.cs, keyword.control.flow.break.cs",
      "settings": {
        "foreground": "#F2969D"
      }
    },
    {
      "scope": "entity.name.function.cs, keyword.operator.arrow.cs, variable.other.object.cs",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": "storage.modifier.cs, keyword.other.class.cs, storage.modifier.cs",
      "settings": {
        "foreground": "#7F7F7F"
      }
    },
    {
      "scope": "storage.type.cs, entity.name.type.enum.cs",
      "settings": {
        "foreground": "#FFDE9E"
      }
    },
    // CSS
    {
      "scope": "keyword.other.important.css",
      "settings": {
        "foreground": "#F2969D"
      }
    },
    {
      "scope": "keyword.other.unit.px.css, keyword.other.unit.em.css, keyword.other.unit.percentage.css,keyword.other.unit.cm.css, keyword.other.unit.mm.css, keyword.other.unit.in.css,keyword.other.unit.pt.css, keyword.other.unit.pc.css, keyword.other.unit.ex.css, keyword.other.unit.ch.css, keyword.other.unit.rem.css, keyword.other.unit.vw.css, keyword.other.unit.vh.css, keyword.other.unit.vmin.css, keyword.other.unit.vmax.css",
      "settings": {
        "foreground": "#A6CEA1"
      }
    },
    {
      "scope": "entity.other.attribute-name.class, entity.other.attribute-name.id",
      "settings": {
        "foreground": "#A6CEA1",
        "fontStyle": ""
      }
    },
    {
      "name": "css property-name",
      "scope": "support.type.vendored.property-name.css",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "support.constant.vendored.property-value.css",
      "scope": "support.constant.vendored.property-value.css",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "text.html.basic entity.other.attribute-name.html",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "",
      "scope": "variable.language",
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
